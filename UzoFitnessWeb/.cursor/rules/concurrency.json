{
  "ownership": {
    "src/data": ["agent-b"],
    "src/routes": ["agent-c", "agent-d"],
    "src/components": ["agent-e"],
    "src/styles": ["agent-e"],
    "src/sw": ["agent-g"],
    "root-config": ["agent-a", "agent-h"]
  },
  "branching": {
    "protected": ["main"],
    "naming": "feat/<workstream>-<short-task>",
    "require_rebase_daily": true
  },
  "gates": {
    "typecheck": true,
    "lint": true,
    "unit": true,
    "e2e_smoke": true,
    "bundle_budget_kb": 350
  },
  "forbiddenImports": [
    {
      "from": "src/routes/*",
      "to": ["src/routes/*/*"]
    },
    {
      "from": "src/routes/*",
      "to": ["src/data/memory/*"]
    }
  ],
  "notifications": {
    "contract_changes": "agent-b must post ADR for any interface changes"
  }
}
